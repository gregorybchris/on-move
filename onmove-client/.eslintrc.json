        "sourceType": "module",
        "ecmaVersion": 7
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "double"]
    },
    "extends": "eslint:recommended"
}